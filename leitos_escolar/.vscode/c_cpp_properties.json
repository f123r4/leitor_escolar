{
    "configurations": [
        {
            "name": "Pico",
            "includePath": [
                "${workspaceFolder}/**",
                "${env:HOME}/.pico-sdk/sdk/2.2.0/**",
                "${env:PICO_SDK_PATH}/**",
                "${workspaceFolder}/build/**",
                "${workspaceFolder}/build/generated/**"
            ],
            "forcedInclude": [
                "${workspaceFolder}/build/generated/pico_base/pico/config_autogen.h",
                "${env:HOME}/.pico-sdk/sdk/2.2.0/src/common/pico_base_headers/include/pico.h"
            ],
            "defines": [],
            "compilerPath": "${env:HOME}/.pico-sdk/toolchain/14_2_Rel1/bin/arm-none-eabi-gcc",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "cStandard": "c17",
            "cppStandard": "c++14",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}
